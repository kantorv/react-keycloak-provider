{
  "name": "react-tsup-package",
  "version": "1.0.4",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsup src/",
    "start": "npm run build -- --watch",
    "type-check": "tsc",
    "test": "jest --config jestconfig.json"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "treeshake": true,
    "sourcemap": "inline",
    "minify": true,
    "clean": true,
    "dts": true,
    "splitting": false,
    "format": [
      "cjs",
      "esm"
    ],
    "external": [
      "react"
    ],
    "injectStyle": false
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "jest": "^29.6.4",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "keycloak-js": "^22.0.1"
  }
}
